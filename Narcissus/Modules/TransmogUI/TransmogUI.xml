<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Script file="Preload.lua"/>

	<Script file="MoneyDisplay.lua"/>
	<Script file="CustomSetsMenu.lua"/>
	<Script file="CustomSetsTab.lua"/>

	<Script file="TransmogUI.lua"/>


	<Button name="NarciCustomSetsMenuButtonTemplate" motionScriptsWhileDisabled="true" virtual="true">
		<Size x="128" y="20"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Texture1" hidden="true">
					<Size x="16" y="16"/>
					<Anchors>
						<Anchor point="LEFT" x="10"/>
					</Anchors>
				</Texture>

				<FontString parentKey="ButtonText" inherits="GameFontNormal" maxLines="1" text="Current Character">
					<Size y="20"/>
					<Anchors>
						<Anchor point="LEFT" x="10"/>
					</Anchors>
				</FontString>

				<FontString parentKey="RightText" inherits="GameFontNormal" maxLines="1" justifyH="RIGHT">
					<Size y="20"/>
					<Color r="0.5" g="0.5" b="0.5"/>
					<Anchors>
						<Anchor point="RIGHT" x="-10"/>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="OVERLAY" textureSubLevel="1">
				<Texture parentKey="Texture2" hidden="true">
					<Size x="16" y="16"/>
					<Anchors>
						<Anchor point="LEFT" x="10"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Button>
</Ui>