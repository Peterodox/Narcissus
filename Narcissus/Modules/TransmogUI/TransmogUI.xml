<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<Script file="Preload.lua"/>

	<Script file="StaticPopup.lua"/>
	<Script file="MoneyDisplay.lua"/>
	<Script file="IconSelector.lua"/>
	<Script file="CustomSetsMenu.lua"/>
	<Script file="CustomSetsTab.lua"/>
	<Script file="SlashCommand.lua"/>

	<Script file="TransmogUI.lua"/>

	<Script file="WardrobeCollection.lua"/>


	<Button name="NarciCustomSetsMenuButtonTemplate" motionScriptsWhileDisabled="true" registerForClicks="LeftButtonUp, RightButtonUp" virtual="true">
		<Size x="128" y="24"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Texture1" hidden="true">
					<Size x="16" y="16"/>
					<Anchors>
						<Anchor point="LEFT" x="10"/>
					</Anchors>
				</Texture>

				<FontString parentKey="ButtonText" inherits="GameFontNormal" maxLines="1" justifyH="LEFT">
					<Size y="20"/>
					<Anchors>
						<Anchor point="LEFT" x="10"/>
					</Anchors>
				</FontString>

				<FontString parentKey="RightText" inherits="GameFontNormal" maxLines="1" justifyH="RIGHT">
					<Size y="20"/>
					<Color r="0.5" g="0.5" b="0.5"/>
					<Anchors>
						<Anchor point="RIGHT" x="-10"/>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="OVERLAY" textureSubLevel="1">
				<Texture parentKey="Texture2" hidden="true">
					<Size x="16" y="16"/>
					<Anchors>
						<Anchor point="LEFT" x="10"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Button>

	<EditBox name="NarciCustomSetsMenuSearchBoxTemplate" inherits="InputBoxInstructionsNineSliceTemplate" autoFocus="false" virtual="true">
		<Size x="128" y="24"/>
		<FontString inherits="GameFontHighlightSmall" justifyH="LEFT"/>
		<TextInsets left="22" right="24" bottom="0" top="0"/>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture parentKey="Left">
					<Size x="12" y="24"/>
					<Anchors>
						<Anchor point="LEFT"/>
					</Anchors>
				</Texture>
				<Texture parentKey="Right">
					<Size x="12" y="24"/>
					<Anchors>
						<Anchor point="RIGHT"/>
					</Anchors>
				</Texture>
				<Texture parentKey="Center">
					<Size y="24"/>
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Left" relativePoint="TOPRIGHT"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Right" relativePoint="BOTTOMLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString parentKey="Instructions" justifyH="LEFT" justifyV="MIDDLE" inherits="GameFontDisableSmall">
					<Anchors>
						<Anchor point="TOPLEFT" x="22" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="-22" y="0"/>
					</Anchors>
					<Color r="0.5" g="0.5" b="0.5"/>
				</FontString>
				<Texture parentKey="SearchIcon">
					<size x="24" y="24"/>
					<Anchors>
						<Anchor point="LEFT" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Button parentKey="ClearButton" hidden="true">
				<Size x="24" y="24"/>
				<Anchors>
					<Anchor point="RIGHT"/>
				</Anchors>
				<Layers>
					<Layer level="OVERLAY">
						<Texture parentKey="Icon" snapToPixelGrid="false" texelSnappingBias="0.0">
							<size x="24" y="24"/>
							<Anchors>
								<Anchor point="LEFT" x="0" y="0"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="HIGHLIGHT">
						<Texture parentKey="Highlight" alphaMode="ADD" snapToPixelGrid="false" texelSnappingBias="0.0">
							<size x="24" y="24"/>
							<Anchors>
								<Anchor point="LEFT" x="0" y="0"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
			</Button>
		</Frames>
	</EditBox>

	<Button name="NarciSelectorIconButtonTemplate" virtual="true">
		<Size x="36" y="36"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Icon">
					<Size x="36" y="36"/>
					<Anchors>
						<Anchor point="CENTER" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY" textureSubLevel="1">
				<Texture parentKey="SelectedTexture" alphaMode="ADD" file="Interface\Buttons\CheckButtonHilight" hidden="true"/>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture parentKey="Highlight" alphaMode="ADD" file="Interface\Buttons\ButtonHilight-Square"/>
			</Layer>
		</Layers>
	</Button>
</Ui>
