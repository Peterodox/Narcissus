	<Button name="Narci_SavedSpellVisualButton_EditButtonTemplate" virtual="true">
		<Size x="8" y="12"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture file="Interface/AddOns/Narcissus/Art/Widgets/SpellVisualBrowser/Icons.tga" parentKey="Icon" Alpha="0.4">
					<Size x="16" y="16"/>
					<TexCoords left="0" right="0" top="0" bottom="0"/>
					<Anchors>
						<Anchor point="CENTER" relativeKey="$parent" relativePoint="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnEnter>
				self.Icon:SetAlpha(1);
				NarciTooltip:ShowTooltip(self, nil, nil, 1);
			</OnEnter>
			<OnLeave>
				NarciTooltip:JustHide();
				self.Icon:SetAlpha(0.4);
			</OnLeave>
			<OnMouseDown>
				NarciTooltip:JustHide();
				self.Icon:SetSize(14, 14);
			</OnMouseDown>
			<OnMouseUp>
				self.Icon:SetSize(16, 16);
			</OnMouseUp>
			<OnDoubleClick>
				return;
			</OnDoubleClick>
