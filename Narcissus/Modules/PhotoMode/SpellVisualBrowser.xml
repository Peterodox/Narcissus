	</Button>

	<Button name="Narci_SavedSpellVisualButtonTemplate" inherits="Narci_SpellVisualButtonSharedTemplate" motionScriptsWhileDisabled="true" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<Texture setAllPoints="true" parentKey="Green" Alpha="0">
					<Color r="0.48627" g="0.77255" b="0.46275" a="0.5"/>
					<Animations>
						<AnimationGroup parentKey="animIn">
							<Alpha fromAlpha="1" toAlpha="1" duration="0.25" order="1"/>
							<Scale fromScaleX="0" toScaleX="1" fromScaleY="1" toScaleY="1" duration="0.25" order="1">
								<Origin point="LEFT">
									<Offset x="0" y="0"/>
								</Origin>
							</Scale>
							<Alpha startDelay="1" fromAlpha="1" toAlpha="0" duration="1" order="2"/>
						</AnimationGroup>
					</Animations>
				</Texture>
			</Layer>
			<Layer level="OVERLAY" textureSubLevel="1">
				<Texture parentKey="Overlay" setAllPoints="true" hidden="true"/>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				local a = 44/255;
				self.Overlay:SetColorTexture(a, a, a, 1);
			</OnLoad>
			<OnDisable>
				self.Icon:SetDesaturated(true);
				self:SetAlpha(0.4);
			</OnDisable>
			<OnEnable>
				self.Icon:SetDesaturated(false);
				self:SetAlpha(1);
			</OnEnable>
		</Scripts>
	</Button>

