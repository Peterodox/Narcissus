	<Frame name="NarciBarberShopStrokeFrameNoScriptTemplate" mixin="NarciBarberShopSharedTemplateMixin" virtual="true">
		<Size x="80" y="20"/>
		<Layers>
			<Layer level="BORDER">
				<Texture parentKey="Border">
					<Color r="0.5" g="0.5" b="0.5"/>
				</Texture>
				<MaskTexture parentKey="Exclusion" hWrapMode="CLAMPTOWHITE" vWrapMode="CLAMPTOWHITE">
					<Anchors>
						<Anchor point="TOPLEFT" x="1" y="-1"/>
						<Anchor point="BOTTOMRIGHT" x="-1" y="1"/>
					</Anchors>
					<MaskedTextures>
						<MaskedTexture childKey="Border"/>
					</MaskedTextures>
				</MaskTexture>
			</Layer>
			<Layer level="BACKGROUND">
				<Texture parentKey="Background">
					<Color r="0" g="0" b="0"/>
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Exclusion" relativePoint="TOPLEFT"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Exclusion" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad method="UpdatePixel"/>
		</Scripts>
	</Frame>

	<Frame name="NarciBarberShopStrokeFrameTemplate" mixin="NarciBarberShopSharedTemplateMixin" inherits="NarciBarberShopStrokeFrameNoScriptTemplate" virtual="true">
		<Scripts>
			<OnLoad method="UpdatePixel"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
		</Scripts>
	</Frame>

